<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <rect width="800" height="600" fill="#f8f9fa"/>
  
  <!-- Background pattern -->
  <defs>
    <pattern id="grid" width="40" height="40" patternUnits="userSpaceOnUse">
      <path d="M 40 0 L 0 0 0 40" fill="none" stroke="#e2e8f0" stroke-width="1"/>
    </pattern>
  </defs>
  <rect width="800" height="600" fill="url(#grid)" opacity="0.5"/>
  
  <text x="400" y="180" font-family="Arial" font-size="42" text-anchor="middle" fill="#0072ce" font-weight="bold">News & Insights</text>
  <text x="400" y="230" font-family="Arial" font-size="24" text-anchor="middle" fill="#6B7280">Latest Industry Updates and Company News</text>
  
  <!-- News elements -->
  <g>
    <!-- Newspaper -->
    <rect x="250" y="300" width="120" height="160" rx="10" fill="white" stroke="#0072ce" stroke-width="2"/>
    <rect x="260" y="310" width="100" height="20" fill="#e6f0ff"/>
    <rect x="260" y="340" width="80" height="10" fill="#cbd5e1"/>
    <rect x="260" y="355" width="100" height="10" fill="#e2e8f0"/>
    <rect x="260" y="370" width="70" height="10" fill="#e2e8f0"/>
    <rect x="260" y="390" width="90" height="10" fill="#e2e8f0"/>
    <rect x="260" y="405" width="60" height="10" fill="#e2e8f0"/>
    <rect x="260" y="425" width="85" height="10" fill="#e2e8f0"/>
    <rect x="260" y="440" width="75" height="10" fill="#e2e8f0"/>
  </g>
  
  <!-- Second newspaper -->
  <g transform="rotate(10, 450, 380)">
    <rect x="420" y="300" width="120" height="160" rx="10" fill="white" stroke="#f97316" stroke-width="2"/>
    <rect x="430" y="310" width="100" height="20" fill="#ffedd5"/>
    <rect x="430" y="340" width="80" height="10" fill="#fed7aa"/>
    <rect x="430" y="355" width="100" height="10" fill="#fed7aa"/>
    <rect x="430" y="370" width="70" height="10" fill="#fed7aa"/>
    <rect x="430" y="390" width="90" height="10" fill="#fed7aa"/>
  </g>
  
  <!-- Article elements -->
  <g>
    <rect x="580" y="300" width="180" height="10" fill="#cbd5e1" rx="5"/>
    <rect x="580" y="320" width="140" height="8" fill="#e2e8f0" rx="4"/>
    <rect x="580" y="335" width="160" height="8" fill="#e2e8f0" rx="4"/>
    <rect x="580" y="350" width="120" height="8" fill="#e2e8f0" rx="4"/>
    <text x="670" y="390" font-family="Arial" font-size="16" text-anchor="middle" fill="#0072ce">Industry</text>
    <text x="670" y="410" font-family="Arial" font-size="16" text-anchor="middle" fill="#f97316">Trends</text>
  </g>
  
  <!-- Icons -->
  <g fill="#0072ce">
    <!-- Megaphone -->
    <path d="M 80 280 L 120 260 L 120 240 L 80 220 Z"/>
    <path d="M 120 260 L 140 270 L 140 230 L 120 240 Z"/>
    <path d="M 140 250 L 160 250 L 160 250 L 140 250"/>
  </g>
  
  <g fill="#f97316">
    <!-- Bell -->
    <path d="M 700 270 Q 720 250 740 270 Q 740 290 720 295 L 720 310 Q 715 315 705 315 Q 695 315 690 310 L 690 295 Q 700 290 700 270 Z"/>
    <circle cx="720" cy="265" r="5" fill="#f97316"/>
  </g>
  
  <!-- Calendar -->
  <rect x="600" y="100" width="40" height="40" fill="white" stroke="#10b981" stroke-width="2"/>
  <rect x="600" y="95" width="40" height="10" fill="#10b981"/>
  <rect x="610" y="110" width="8" height="8" fill="#10b981"/>
  <rect x="622" y="110" width="8" height="8" fill="#10b981"/>
  <rect x="610" y="122" width="8" height="8" fill="#10b981"/>
  
  <!-- Timestamp -->
  <g fill="#6B7280">
    <circle cx="700" cy="120" r="20" fill="white" stroke="#6B7280" stroke-width="2"/>
    <line x1="700" y1="120" x2="700" y2="135"/>
    <line x1="700" y1="120" x2="710" y2="120"/>
  </g>
  
  <!-- Technology elements -->
  <g>
    <!-- Chip -->
    <rect x="100" y="450" width="30" height="30" fill="#e6f0ff" stroke="#0072ce" stroke-width="2"/>
    <rect x="105" y="455" width="6" height="6" fill="#f97316"/>
    <rect x="119" y="455" width="6" height="6" fill="#f97316"/>
    <rect x="105" y="469" width="6" height="6" fill="#f97316"/>
    <rect x="119" y="469" width="6" height="6" fill="#f97316"/>
    
    <!-- Power arrow -->
    <rect x="140" y="465" width="4" height="20" fill="#f97316"/>
    <polygon points="138,465 142,465 140,455" fill="#f97316"/>
    
    <!-- Waveform -->
    <path d="M 150 470 Q 160 460 170 470 Q 180 480 190 470 Q 200 460 210 470" stroke="#0072ce" stroke-width="3" fill="none"/>
  </g>
  
  <!-- Arrows pointing to news items -->
  <g fill="#0072ce">
    <polygon points="350,280 360,260 370,280"/>
    <polygon points="500,280 510,260 520,280"/>
    <polygon points="650,280 660,260 670,280"/>
  </g>
  
  <!-- News tags -->
  <g>
    <rect x="350" y="480" width="100" height="25" rx="12" fill="#e6f0ff" stroke="#0072ce" stroke-width="1"/>
    <rect x="460" y="480" width="80" height="25" rx="12" fill="#ffedd5" stroke="#f97316" stroke-width="1"/>
    <rect x="550" y="480" width="90" height="25" rx="12" fill="#e7f5e5" stroke="#10b981" stroke-width="1"/>
    
    <text x="400" y="497" font-family="Arial" font-size="12" text-anchor="middle" fill="#0072ce">Technology</text>
    <text x="500" y="497" font-family="Arial" font-size="12" text-anchor="middle" fill="#f97316">Market</text>
    <text x="595" y="497" font-family="Arial" font-size="12" text-anchor="middle" fill="#10b981">Innovation</text>
  </g>
</svg>