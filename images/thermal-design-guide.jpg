<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <rect width="800" height="600" fill="#f0f9ff"/>
  
  <!-- Background pattern -->
  <defs>
    <pattern id="grid" width="40" height="40" patternUnits="userSpaceOnUse">
      <path d="M 40 0 L 0 0 0 40" fill="none" stroke="#e2e8f0" stroke-width="1"/>
    </pattern>
  </defs>
  <rect width="800" height="600" fill="url(#grid)" opacity="0.3"/>
  
  <text x="400" y="180" font-family="Arial" font-size="32" text-anchor="middle" fill="#0072ce">Thermal Design Guidelines</text>
  <text x="400" y="230" font-family="Arial" font-size="18" text-anchor="middle" fill="#6B7280">Best Practices for Power Module Heat Management</text>
  
  <!-- Heat flow visualization -->
  <g>
    <!-- Power module representation -->
    <rect x="300" y="280" width="200" height="120" rx="15" fill="#e6f0ff" stroke="#0072ce" stroke-width="3"/>
    <text x="400" y="315" font-family="Arial" font-size="14" text-anchor="middle" fill="#0072ce">Vicor Module</text>
    
    <!-- Thermal zones -->
    <rect x="320" y="320" width="60" height="20" fill="#f97316" opacity="0.3"/>
    <rect x="420" y="320" width="60" height="20" fill="#f97316" opacity="0.3"/>
    
    <!-- Heat flow arrows -->
    <g fill="#f97316">
      <polygon points="400,400 390,380 410,380"/> <!-- Downward arrow -->
      <polygon points="340,340 320,330 320,350"/> <!-- Left arrow -->
      <polygon points="460,340 480,330 480,350"/> <!-- Right arrow -->
      <polygon points="400,280 390,300 410,300"/> <!-- Upward arrow -->
    </g>
    
    <!-- Heat dissipation -->
    <circle cx="360" cy="420" r="15" fill="#f97316" opacity="0.2"/>
    <circle cx="440" cy="420" r="20" fill="#f97316" opacity="0.2"/>
    <circle cx="400" cy="450" r="25" fill="#f97316" opacity="0.2"/>
  </g>
  
  <!-- Thermal vias representation -->
  <g fill="#0072ce">
    <circle cx="310" cy="410" r="3"/>
    <circle cx="320" cy="420" r="3"/>
    <circle cx="315" cy="435" r="3"/>
    <circle cx="325" cy="445" r="3"/>
    <circle cx="335" cy="425" r="3"/>
    <circle cx="340" cy="440" r="3"/>
    <circle cx="350" cy="430" r="3"/>
    <circle cx="360" cy="445" r="3"/>
    <circle cx="370" cy="415" r="3"/>
    <circle cx="380" cy="435" r="3"/>
    <circle cx="390" cy="425" r="3"/>
    <circle cx="400" cy="440" r="3"/>
    <circle cx="410" cy="410" r="3"/>
    <circle cx="420" cy="420" r="3"/>
    <circle cx="430" cy="430" r="3"/>
    <circle cx="440" cy="440" r="3"/>
    <circle cx="450" cy="425" r="3"/>
    <circle cx="460" cy="415" r="3"/>
    <circle cx="470" cy="435" r="3"/>
    <circle cx="465" cy="445" r="3"/>
  </g>
  
  <!-- Heat sink -->
  <rect x="250" y="460" width="300" height="20" fill="#94a3b8"/>
  <rect x="260" y="450" width="10" height="30" fill="#64748b"/>
  <rect x="280" y="450" width="10" height="30" fill="#64748b"/>
  <rect x="300" y="450" width="10" height="30" fill="#64748b"/>
  <rect x="320" y="450" width="10" height="30" fill="#64748b"/>
  <rect x="340" y="450" width="10" height="30" fill="#64748b"/>
  <rect x="360" y="450" width="10" height="30" fill="#64748b"/>
  <rect x="380" y="450" width="10" height="30" fill="#64748b"/>
  <rect x="400" y="450" width="10" height="30" fill="#64748b"/>
  <rect x="420" y="450" width="10" height="30" fill="#64748b"/>
  <rect x="440" y="450" width="10" height="30" fill="#64748b"/>
  <rect x="460" y="450" width="10" height="30" fill="#64748b"/>
  <rect x="480" y="450" width="10" height="30" fill="#64748b"/>
  <text x="400" y="495" font-family="Arial" font-size="14" text-anchor="middle" fill="#6B7280">Heat Sink</text>
  
  <!-- Thermal management elements -->
  <g>
    <!-- Temperature monitor -->
    <rect x="100" y="150" width="120" height="60" fill="#e6f0ff" stroke="#0072ce" stroke-width="2" rx="5"/>
    <text x="160" y="170" font-family="Arial" font-size="12" text-anchor="middle" fill="#0072ce">Temperature</text>
    <text x="160" y="185" font-family="Arial" font-size="14" text-anchor="middle" fill="#f97316">75Â°C</text>
    
    <!-- Thermal interface -->
    <rect x="580" y="300" width="100" height="30" fill="#fef3c7" stroke="#f59e0b" stroke-width="2" rx="5"/>
    <text x="630" y="320" font-family="Arial" font-size="12" text-anchor="middle" fill="#92400e">TIM Layer</text>
    
    <!-- Fan -->
    <circle cx="650" cy="150" r="40" fill="#e6f0ff" stroke="#0072ce" stroke-width="2"/>
    <circle cx="650" cy="150" r="15" fill="#0072ce"/>
    
    <!-- Fan blades -->
    <line x1="650" y1="110" x2="650" y2="190" stroke="#0072ce" stroke-width="2"/>
    <line x1="610" y1="150" x2="690" y2="150" stroke="#0072ce" stroke-width="2"/>
    <line x1="624" y1="124" x2="676" y2="176" stroke="#0072ce" stroke-width="2"/>
    <line x1="624" y1="176" x2="676" y2="124" stroke="#0072ce" stroke-width="2"/>
    <text x="650" y="100" font-family="Arial" font-size="12" text-anchor="middle" fill="#6B7280">Forced Air</text>
  </g>
  
  <!-- Thermal design principles -->
  <g>
    <rect x="100" y="250" width="150" height="80" fill="#e7f5e5" stroke="#10b981" stroke-width="2" rx="5"/>
    <text x="175" y="270" font-family="Arial" font-size="12" text-anchor="middle" fill="#065f46">Principle 1</text>
    <text x="175" y="290" font-family="Arial" font-size="11" text-anchor="middle" fill="#065f46">Maximize</text>
    <text x="175" y="305" font-family="Arial" font-size="11" text-anchor="middle" fill="#065f46">Thermal Path</text>
    
    <rect x="550" y="250" width="150" height="80" fill="#e7f5e5" stroke="#10b981" stroke-width="2" rx="5"/>
    <text x="625" y="270" font-family="Arial" font-size="12" text-anchor="middle" fill="#065f46">Principle 2</text>
    <text x="625" y="290" font-family="Arial" font-size="11" text-anchor="middle" fill="#065f46">Minimize</text>
    <text x="625" y="305" font-family="Arial" font-size="11" text-anchor="middle" fill="#065f46">Thermal Resistance</text>
    
    <rect x="275" y="100" width="250" height="60" fill="#eff6ff" stroke="#3b82f6" stroke-width="2" rx="5"/>
    <text x="395" y="120" font-family="Arial" font-size="12" text-anchor="middle" fill="#1d4ed8">Thermal Management Strategy</text>
    <text x="395" y="135" font-family="Arial" font-size="10" text-anchor="middle" fill="#1d4ed8">Optimize Heat Transfer Through Proper Design</text>
  </g>
  
  <!-- Legend -->
  <g font-family="Arial" font-size="12" fill="#6B7280">
    <rect x="100" y="500" width="20" height="20" fill="#f97316" opacity="0.3"/>
    <text x="130" y="515">Heat Source</text>
    
    <rect x="250" y="500" width="20" height="20" fill="#0072ce"/>
    <text x="280" y="515">Thermal Vias</text>
    
    <rect x="400" y="500" width="20" height="20" fill="#94a3b8"/>
    <text x="430" y="515">Heat Spreaders</text>
  </g>
</svg>