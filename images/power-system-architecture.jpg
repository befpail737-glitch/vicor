<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#f0f9ff"/>
      <stop offset="100%" stop-color="#e0f2fe"/>
    </linearGradient>
  </defs>
  
  <rect width="800" height="600" fill="url(#bgGradient)"/>
  
  <!-- Background pattern -->
  <defs>
    <pattern id="grid" width="40" height="40" patternUnits="userSpaceOnUse">
      <path d="M 40 0 L 0 0 0 40" fill="none" stroke="#bae6fd" stroke-width="0.5" opacity="0.3"/>
    </pattern>
  </defs>
  <rect width="800" height="600" fill="url(#grid)"/>
  
  <!-- Power flow visualization -->
  <g opacity="0.8">
    <!-- Main power rail -->
    <rect x="100" y="100" width="600" height="40" rx="20" fill="#e0f2fe" stroke="#0072ce" stroke-width="2"/>
    <text x="400" y="128" font-family="Arial" font-size="16" text-anchor="middle" fill="#0072ce">Power Distribution Network</text>
    
    <!-- Branches to components -->
    <g stroke="#0072ce" stroke-width="3">
      <line x1="200" y1="140" x2="200" y2="200"/>
      <line x1="300" y1="140" x2="300" y2="200"/>
      <line x1="400" y1="140" x2="400" y2="200"/>
      <line x1="500" y1="140" x2="500" y2="200"/>
      <line x1="600" y1="140" x2="600" y2="200"/>
    </g>
    
    <!-- Components -->
    <g>
      <!-- CPU Power Module -->
      <rect x="180" y="200" width="40" height="80" rx="5" fill="#d1fae5" stroke="#10b981" stroke-width="2"/>
      <text x="200" y="235" font-family="Arial" font-size="10" text-anchor="middle" fill="#065f46">CPU</text>
      <text x="200" y="250" font-family="Arial" font-size="8" text-anchor="middle" fill="#065f46">12V→0.8V</text>
      <text x="200" y="260" font-family="Arial" font-size="8" text-anchor="middle" fill="#065f46">@200A</text>
      
      <!-- GPU Power Module -->
      <rect x="280" y="200" width="40" height="80" rx="5" fill="#fef08a" stroke="#facc15" stroke-width="2"/>
      <text x="300" y="235" font-family="Arial" font-size="10" text-anchor="middle" fill="#92400e">GPU</text>
      <text x="300" y="250" font-family="Arial" font-size="8" text-anchor="middle" fill="#92400e">12V→0.9V</text>
      <text x="300" y="260" font-family="Arial" font-size="8" text-anchor="middle" fill="#92400e">@150A</text>
      
      <!-- Memory Power Module -->
      <rect x="380" y="200" width="40" height="80" rx="5" fill="#fee2e2" stroke="#f87171" stroke-width="2"/>
      <text x="400" y="235" font-family="Arial" font-size="10" text-anchor="middle" fill="#b91c1c">MEM</text>
      <text x="400" y="250" font-family="Arial" font-size="8" text-anchor="middle" fill="#b91c1c">12V→1.2V</text>
      <text x="400" y="260" font-family="Arial" font-size="8" text-anchor="middle" fill="#b91c1c">@50A</text>
      
      <!-- Storage Power Module -->
      <rect x="480" y="200" width="40" height="80" rx="5" fill="#ede9fe" stroke="#a78bfa" stroke-width="2"/>
      <text x="500" y="235" font-family="Arial" font-size="10" text-anchor="middle" fill="#5b21b6">STG</text>
      <text x="500" y="250" font-family="Arial" font-size="8" text-anchor="middle" fill="#5b21b6">12V→5V</text>
      <text x="500" y="260" font-family="Arial" font-size="8" text-anchor="middle" fill="#5b21b6">@10A</text>
      
      <!-- Peripheral Power Module -->
      <rect x="580" y="200" width="40" height="80" rx="5" fill="#e0e7ff" stroke="#818cf8" stroke-width="2"/>
      <text x="600" y="235" font-family="Arial" font-size="10" text-anchor="middle" fill="#4f46e5">PER</text>
      <text x="600" y="250" font-family="Arial" font-size="8" text-anchor="middle" fill="#4f46e5">12V→3.3V</text>
      <text x="600" y="260" font-family="Arial" font-size="8" text-anchor="middle" fill="#4f46e5">@20A</text>
    </g>
    
    <!-- Power flow arrows -->
    <g fill="#f97316">
      <polygon points="500,90 490,110 510,110"/> <!-- 48V to rail -->
      <polygon points="180,190 170,170 190,170"/> <!-- CPU power -->
      <polygon points="280,190 270,170 290,170"/> <!-- GPU power -->
      <polygon points="380,190 370,170 390,170"/> <!-- Mem power -->
      <polygon points="480,190 470,170 490,170"/> <!-- Storage power -->
      <polygon points="580,190 570,170 590,170"/> <!-- Periph power -->
    </g>
    
    <!-- High Power Modules -->
    <g>
      <!-- PRM Module -->
      <rect x="150" y="320" width="80" height="50" rx="8" fill="#a5f3fc" stroke="#06b6d4" stroke-width="2"/>
      <text x="190" y="345" font-family="Arial" font-size="12" text-anchor="middle" fill="#0e7490">PRM</text>
      <text x="190" y="360" font-family="Arial" font-size="10" text-anchor="middle" fill="#0e7490">Regulator</text>
      
      <!-- VTM Module -->
      <rect x="250" y="320" width="80" height="50" rx="8" fill="#a5f3fc" stroke="#06b6d4" stroke-width="2"/>
      <text x="290" y="345" font-family="Arial" font-size="12" text-anchor="middle" fill="#0e7490">VTM</text>
      <text x="290" y="360" font-family="Arial" font-size="10" text-anchor="middle" fill="#0e7490">Transformer</text>
      
      <!-- BCM Module -->
      <rect x="350" y="320" width="80" height="50" rx="8" fill="#a5f3fc" stroke="#06b6d4" stroke-width="2"/>
      <text x="390" y="345" font-family="Arial" font-size="12" text-anchor="middle" fill="#0e7490">BCM</text>
      <text x="390" y="360" font-family="Arial" font-size="10" text-anchor="middle" fill="#0e7490">Multiplier</text>
      
      <!-- DCM Module -->
      <rect x="450" y="320" width="80" height="50" rx="8" fill="#a5f3fc" stroke="#06b6d4" stroke-width="2"/>
      <text x="490" y="345" font-family="Arial" font-size="12" text-anchor="middle" fill="#0e7490">DCM</text>
      <text x="490" y="360" font-family="Arial" font-size="10" text-anchor="middle" fill="#0e7490">Converter</text>
    </g>
    
    <!-- Connection lines -->
    <g stroke="#0072ce" stroke-width="2">
      <line x1="190" y1="320" x2="190" y2="280" stroke-dasharray="5,5"/>
      <line x1="290" y1="320" x2="290" y2="280" stroke-dasharray="5,5"/>
      <line x1="390" y1="320" x2="390" y2="280" stroke-dasharray="5,5"/>
      <line x1="490" y1="320" x2="490" y2="280" stroke-dasharray="5,5"/>
    </g>
    
    <!-- Efficiency indicator -->
    <circle cx="700" cy="150" r="40" fill="#e7f5e5" stroke="#10b981" stroke-width="3"/>
    <text x="700" y="145" font-family="Arial" font-size="14" text-anchor="middle" fill="#065f46">High</text>
    <text x="700" y="165" font-family="Arial" font-size="14" text-anchor="middle" fill="#065f46">Efficiency</text>
    <text x="700" y="185" font-family="Arial" font-size="12" text-anchor="middle" fill="#065f46">&gt;96%</text>
  </g>
  
  <!-- Title -->
  <text x="400" y="60" font-family="Arial" font-size="24" text-anchor="middle" fill="#0072ce">Power System Architecture</text>
  <text x="400" y="85" font-family="Arial" font-size="16" text-anchor="middle" fill="#6B7280">Vicor Power Conversion Solutions</text>
  
  <!-- Power levels -->
  <g font-family="Arial" font-size="10" text-anchor="middle" fill="#6B7280">
    <text x="200" y="295">400W</text>
    <text x="300" y="295">270W</text>
    <text x="400" y="295">120W</text>
    <text x="500" y="295">50W</text>
    <text x="600" y="295">66W</text>
  </g>
</svg>