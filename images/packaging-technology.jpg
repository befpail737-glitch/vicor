<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <rect width="800" height="600" fill="#f8f9fa"/>
  <text x="400" y="180" font-family="Arial" font-size="36" text-anchor="middle" fill="#0072ce">Packaging Technology</text>
  <text x="400" y="230" font-family="Arial" font-size="24" text-anchor="middle" fill="#6B7280">ChiP, VIA, and Advanced Solutions</text>
  
  <!-- ChiP package representation -->
  <g>
    <rect x="150" y="300" width="120" height="100" rx="10" fill="#e6f0ff" stroke="#0072ce" stroke-width="2"/>
    <text x="210" y="320" font-family="Arial" font-size="14" text-anchor="middle" fill="#0072ce">ChiP</text>
    <text x="210" y="340" font-family="Arial" font-size="12" text-anchor="middle" fill="#6B7280">Converter</text>
    <text x="210" y="355" font-family="Arial" font-size="12" text-anchor="middle" fill="#6B7280">housed</text>
    <text x="210" y="370" font-family="Arial" font-size="12" text-anchor="middle" fill="#6B7280">in Package</text>
    
    <!-- Component inside ChiP -->
    <rect x="170" y="380" width="80" height="10" fill="#f97316"/>
    <circle cx="185" cy="395" r="5" fill="#f97316"/>
    <circle cx="200" cy="395" r="5" fill="#f97316"/>
    <circle cx="215" cy="395" r="5" fill="#f97316"/>
  </g>
  
  <!-- VIA package representation -->
  <g>
    <rect x="340" y="280" width="120" height="20" rx="5" fill="#e6f0ff" stroke="#0072ce" stroke-width="2"/>
    <rect x="390" y="250" width="20" height="30" fill="#e6f0ff" stroke="#0072ce" stroke-width="2"/>
    <text x="400" y="270" font-family="Arial" font-size="14" text-anchor="middle" fill="#0072ce">VIA</text>
    <text x="400" y="295" font-family="Arial" font-size="12" text-anchor="middle" fill="#6B7280">Vertical</text>
    <text x="400" y="310" font-family="Arial" font-size="12" text-anchor="middle" fill="#6B7280">Integration</text>
    <text x="400" y="325" font-family="Arial" font-size="12" text-anchor="middle" fill="#6B7280">Architecture</text>
    
    <!-- Pins -->
    <rect x="350" y="300" width="8" height="15" fill="#f97316"/>
    <rect x="365" y="300" width="8" height="15" fill="#f97316"/>
    <rect x="380" y="300" width="8" height="15" fill="#f97316"/>
    <rect x="395" y="300" width="8" height="15" fill="#f97316"/>
    <rect x="410" y="300" width="8" height="15" fill="#f97316"/>
    <rect x="425" y="300" width="8" height="15" fill="#f97316"/>
    <rect x="440" y="300" width="8" height="15" fill="#f97316"/>
  </g>
  
  <!-- Traditional package representation -->
  <g>
    <rect x="550" y="300" width="120" height="100" rx="10" fill="#e6f0ff" stroke="#0072ce" stroke-width="2"/>
    <text x="610" y="320" font-family="Arial" font-size="14" text-anchor="middle" fill="#0072ce">LGA</text>
    <text x="610" y="340" font-family="Arial" font-size="12" text-anchor="middle" fill="#6B7280">Standard</text>
    <text x="610" y="355" font-family="Arial" font-size="12" text-anchor="middle" fill="#6B7280">Package</text>
    
    <!-- Pins around perimeter -->
    <rect x="545" y="330" width="10" height="8" fill="#f97316"/>
    <rect x="545" y="345" width="10" height="8" fill="#f97316"/>
    <rect x="545" y="360" width="10" height="8" fill="#f97316"/>
    <rect x="665" y="330" width="10" height="8" fill="#f97316"/>
    <rect x="665" y="345" width="10" height="8" fill="#f97316"/>
    <rect x="665" y="360" width="10" height="8" fill="#f97316"/>
    <rect x="570" y="295" width="8" height="10" fill="#f97316"/>
    <rect x="585" y="295" width="8" height="10" fill="#f97316"/>
    <rect x="600" y="295" width="8" height="10" fill="#f97316"/>
    <rect x="615" y="295" width="8" height="10" fill="#f97316"/>
    <rect x="630" y="295" width="8" height="10" fill="#f97316"/>
    <rect x="570" y="400" width="8" height="10" fill="#f97316"/>
    <rect x="585" y="400" width="8" height="10" fill="#f97316"/>
    <rect x="600" y="400" width="8" height="10" fill="#f97316"/>
    <rect x="615" y="400" width="8" height="10" fill="#f97316"/>
    <rect x="630" y="400" width="8" height="10" fill="#f97316"/>
  </g>
  
  <!-- Connection lines showing density -->
  <g stroke="#f97316" stroke-width="2" stroke-dasharray="4,4">
    <line x1="210" y1="400" x2="400" y2="300"/>
    <line x1="400" y1="300" x2="610" y2="400"/>
  </g>
  
  <!-- Density indicator -->
  <text x="300" y="450" font-family="Arial" font-size="16" text-anchor="middle" fill="#f97316">High Density</text>
  <text x="400" y="450" font-family="Arial" font-size="16" text-anchor="middle" fill="#0072ce">Medium</text>
  <text x="500" y="450" font-family="Arial" font-size="16" text-anchor="middle" fill="#6B7280">Standard</text>
  
  <!-- Power arrow -->
  <g fill="#f97316">
    <rect x="370" y="400" width="4" height="60"/>
    <polygon points="366,460 374,460 370,470"/>
    <text x="370" y="500" font-family="Arial" font-size="14" text-anchor="middle" fill="#f97316">Power</text>
  </g>
</svg>