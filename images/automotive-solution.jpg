<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <rect width="800" height="600" fill="#0f172a"/>
  
  <!-- Gradient background -->
  <defs>
    <linearGradient id="automotiveGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#0f172a"/>
      <stop offset="100%" stop-color="#1e293b"/>
    </linearGradient>
  </defs>
  <rect width="800" height="600" fill="url(#automotiveGradient)"/>
  
  <text x="400" y="80" font-family="Arial" font-size="32" text-anchor="middle" fill="#cbd5e1">Automotive Power Solutions</text>
  <text x="400" y="120" font-family="Arial" font-size="18" text-anchor="middle" fill="#94a3b8">High-Efficiency Power for Electric & Hybrid Vehicles</text>
  
  <!-- Electric car -->
  <g>
    <!-- Car body -->
    <rect x="200" y="250" width="400" height="120" rx="20" fill="#e6e7e9" stroke="#cbd5e1" stroke-width="3"/>
    
    <!-- Car hood -->
    <path d="M 200 250 Q 250 200 300 230 Q 350 220 400 240" fill="#e6e7e9" stroke="#cbd5e1" stroke-width="3"/>
    
    <!-- Windshield -->
    <path d="M 300 230 L 450 240 L 440 290 L 290 280 Z" fill="#bae6fd" stroke="#93c5fd" stroke-width="1"/>
    
    <!-- Wheels -->
    <circle cx="260" cy="350" r="30" fill="#1e293b" stroke="#64748b" stroke-width="3"/>
    <circle cx="540" cy="350" r="30" fill="#1e293b" stroke="#64748b" stroke-width="3"/>
    <circle cx="260" cy="350" r="15" fill="#64748b"/>
    <circle cx="540" cy="350" r="15" fill="#64748b"/>
    
    <!-- Lights -->
    <circle cx="190" cy="290" r="8" fill="#f59e0b"/>
    <circle cx="610" cy="290" r="8" fill="#e5e7eb"/>
    
    <!-- License plate -->
    <rect x="550" y="300" width="60" height="30" fill="#64748b" rx="3"/>
    <text x="580" y="320" font-family="Arial" font-size="10" text-anchor="middle" fill="white">ELECTRIC</text>
  </g>
  
  <!-- Powertrain components -->
  <g>
    <!-- Battery pack -->
    <rect x="320" y="390" width="160" height="60" rx="10" fill="#0072ce" stroke="#004a99" stroke-width="2"/>
    <rect x="330" y="400" width="20" height="40" fill="#f97316" rx="3"/>
    <rect x="355" y="400" width="20" height="40" fill="#f97316" rx="3"/>
    <rect x="380" y="400" width="20" height="40" fill="#f97316" rx="3"/>
    <rect x="405" y="400" width="20" height="40" fill="#f97316" rx="3"/>
    <rect x="430" y="400" width="20" height="40" fill="#f97316" rx="3"/>
    <text x="400" y="470" font-family="Arial" font-size="14" text-anchor="middle" fill="#cbd5e1">HV Battery Pack</text>
  </g>
  
  <!-- Power modules -->
  <g>
    <!-- Inverter -->
    <rect x="450" y="250" width="80" height="40" rx="8" fill="#0072ce" stroke="#004a99" stroke-width="2"/>
    <text x="490" y="275" font-family="Arial" font-size="10" text-anchor="middle" fill="white">Inverter</text>
    
    <!-- DC-DC converter -->
    <rect x="250" y="250" width="80" height="40" rx="8" fill="#0072ce" stroke="#004a99" stroke-width="2"/>
    <text x="290" y="275" font-family="Arial" font-size="10" text-anchor="middle" fill="white">DC-DC Conv</text>
    
    <!-- Charger -->
    <rect x="200" y="320" width="60" height="30" rx="5" fill="#0072ce" stroke="#004a99" stroke-width="2"/>
    <text x="230" y="340" font-family="Arial" font-size="10" text-anchor="middle" fill="white">Charger</text>
  </g>
  
  <!-- Power flow -->
  <g stroke="#f97316" stroke-width="3" fill="none">
    <!-- From battery to inverter -->
    <path d="M 400 390 Q 400 350 490 290" stroke-dasharray="5,3"/>
    
    <!-- From battery to DC-DC -->
    <path d="M 380 390 Q 350 350 290 290" stroke-dasharray="5,3"/>
    
    <!-- From battery to charger -->
    <path d="M 350 390 Q 270 360 230 350" stroke-dasharray="5,3"/>
    
    <!-- From DC-DC to 12V system -->
    <line x1="290" y1="290" x2="290" y2="200" stroke-dasharray="5,3"/>
  </g>
  
  <!-- Arrow heads -->
  <g fill="#f97316">
    <polygon points="490,295 480,290 490,285"/>
    <polygon points="290,295 280,290 290,285"/>
    <polygon points="230,355 220,350 230,345"/>
    <polygon points="285,200 290,190 295,200"/>
  </g>
  
  <!-- 12V system components -->
  <g>
    <!-- 12V battery -->
    <rect x="260" y="120" width="60" height="30" fill="#e5e7eb" stroke="#9ca3af" stroke-width="2"/>
    <rect x="265" y="125" width="8" height="20" fill="#f97316"/>
    <rect x="278" y="125" width="8" height="20" fill="#f97316"/>
    <rect x="291" y="125" width="8" height="20" fill="#f97316"/>
    <rect x="304" y="125" width="8" height="20" fill="#f97316"/>
    
    <!-- Headlights -->
    <circle cx="270" cy="100" r="12" fill="#f59e0b" opacity="0.6"/>
    <circle cx="310" cy="100" r="12" fill="#f59e0b" opacity="0.6"/>
    
    <!-- Dashboard -->
    <rect x="340" y="100" width="120" height="30" fill="#374151" rx="5"/>
    <circle cx="360" cy="115" r="5" fill="#10b981"/>
    <circle cx="390" cy="115" r="5" fill="#f59e0b"/>
    <circle cx="420" cy="115" r="5" fill="#ef4444"/>
  </g>
  
  <!-- Vehicle safety -->
  <g>
    <!-- ABS module -->
    <rect x="100" y="200" width="70" height="30" fill="#1e293b" stroke="#64748b" stroke-width="2"/>
    <text x="135" y="220" font-family="Arial" font-size="10" text-anchor="middle" fill="white">ABS</text>
    
    <!-- Airbag system -->
    <rect x="100" y="250" width="70" height="30" fill="#1e293b" stroke="#64748b" stroke-width="2"/>
    <text x="135" y="270" font-family="Arial" font-size="10" text-anchor="middle" fill="white">AIRBAG</text>
    
    <!-- ADAS module -->
    <rect x="100" y="300" width="70" height="30" fill="#1e293b" stroke="#64748b" stroke-width="2"/>
    <text x="135" y="320" font-family="Arial" font-size="10" text-anchor="middle" fill="white">ADAS</text>
  </g>
  
  <!-- Power distribution -->
  <g>
    <!-- Junction box -->
    <rect x="400" y="150" width="80" height="40" fill="#475569" stroke="#94a3b8" stroke-width="2"/>
    <rect x="410" y="160" width="15" height="20" fill="#f97316"/>
    <rect x="430" y="160" width="15" height="20" fill="#f97316"/>
    <rect x="450" y="160" width="15" height="20" fill="#f97316"/>
    
    <!-- Power rail -->
    <rect x="380" y="180" width="120" height="10" fill="#e5e7eb"/>
  </g>
  
  <!-- Efficiency indicators -->
  <g>
    <!-- High efficiency badge -->
    <circle cx="650" cy="200" r="40" fill="#0072ce" stroke="#004a99" stroke-width="3"/>
    <text x="650" y="195" font-family="Arial" font-size="14" text-anchor="middle" fill="white">High</text>
    <text x="650" y="215" font-family="Arial" font-size="14" text-anchor="middle" fill="white">Efficiency</text>
    <text x="650" y="235" font-family="Arial" font-size="12" text-anchor="middle" fill="white">&gt;96%</text>
  </g>
  
  <!-- Thermal management -->
  <g>
    <rect x="580" y="300" width="140" height="40" fill="#e7f5e5" stroke="#10b981" stroke-width="2" rx="5"/>
    <text x="650" y="320" font-family="Arial" font-size="14" text-anchor="middle" fill="#065f46">Advanced</text>
    <text x="650" y="335" font-family="Arial" font-size="14" text-anchor="middle" fill="#065f46">Thermal Mgmt</text>
  </g>
  
  <!-- Safety certified -->
  <g>
    <path d="M 580 400 L 620 380 L 660 400 L 620 420 Z" fill="#10b981" stroke="#065f46" stroke-width="2"/>
    <text x="620" y="450" font-family="Arial" font-size="14" text-anchor="middle" fill="#10b981">AEC-Q100</text>
    <text x="620" y="470" font-family="Arial" font-size="14" text-anchor="middle" fill="#10b981">Qualified</text>
  </g>
  
  <!-- Range extender -->
  <g>
    <rect x="580" y="500" width="140" height="40" fill="#1e40af" stroke="#1e3a8a" stroke-width="2" rx="5"/>
    <text x="650" y="520" font-family="Arial" font-size="14" text-anchor="middle" fill="white">Extended</text>
    <text x="650" y="535" font-family="Arial" font-size="14" text-anchor="middle" fill="white">Driving Range</text>
  </g>
  
  <!-- Automotive standards -->
  <g>
    <circle cx="150" cy="400" r="30" fill="none" stroke="#f97316" stroke-width="3"/>
    <line x1="120" y1="400" x2="180" y2="400" stroke="#f97316" stroke-width="3"/>
    <line x1="150" y1="370" x2="150" y2="430" stroke="#f97316" stroke-width="3"/>
    <text x="150" y="450" font-family="Arial" font-size="12" text-anchor="middle" fill="#f97316">EMC</text>
    <text x="150" y="470" font-family="Arial" font-size="12" text-anchor="middle" fill="#f97316">Compliant</text>
  </g>
  
  <!-- EV charging -->
  <g>
    <rect x="100" y="480" width="100" height="50" fill="#1e293b" stroke="#64748b" stroke-width="2"/>
    <rect x="120" y="495" width="60" height="20" fill="#f59e0b"/>
    <text x="150" y="520" font-family="Arial" font-size="10" text-anchor="middle" fill="white">EV Charger</text>
  </g>
</svg>