<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <rect width="800" height="600" fill="#0c4a6e"/>
  
  <!-- Stars background -->
  <defs>
    <radialGradient id="skyGradient" cx="50%" cy="50%" r="50%" fx="50%" fy="50%">
      <stop offset="0%" stop-color="#0c4a6e"/>
      <stop offset="100%" stop-color="#030712"/>
    </radialGradient>
    <filter id="glow">
      <feGaussianBlur stdDeviation="2" result="coloredBlur"/>
      <feMerge> 
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  <rect width="800" height="600" fill="url(#skyGradient)"/>
  
  <!-- Stars -->
  <g fill="white">
    <circle cx="100" cy="80" r="1.5"/>
    <circle cx="150" cy="120" r="1"/>
    <circle cx="200" cy="60" r="1.2"/>
    <circle cx="300" cy="90" r="1.5"/>
    <circle cx="400" cy="50" r="1"/>
    <circle cx="500" cy="110" r="1.3"/>
    <circle cx="600" cy="70" r="1.2"/>
    <circle cx="700" cy="100" r="1.4"/>
    <circle cx="750" cy="80" r="1"/>
    <circle cx="650" cy="120" r="0.8"/>
    <circle cx="550" cy="150" r="1.1"/>
    <circle cx="450" cy="130" r="1.2"/>
    <circle cx="350" cy="150" r="1.3"/>
    <circle cx="250" cy="140" r="1"/>
    <circle cx="120" cy="150" r="1.2"/>
  </g>
  
  <!-- Moon -->
  <circle cx="700" cy="150" r="30" fill="#e2e8f0"/>
  <circle cx="715" cy="140" r="8" fill="#cbd5e1"/>
  
  <!-- Airplane -->
  <g transform="rotate(-15, 200, 250)">
    <rect x="180" y="240" width="80" height="15" rx="5" fill="#f0f9ff" stroke="#bae6fd" stroke-width="1"/>
    <rect x="220" y="230" width="20" height="10" rx="5" fill="#bae6fd"/>
    <rect x="190" y="245" width="10" height="20" rx="3" fill="#bae6fd"/>
    <rect x="250" y="245" width="10" height="20" rx="3" fill="#bae6fd"/>
    <text x="220" y="275" font-family="Arial" font-size="14" text-anchor="middle" fill="white">Aircraft</text>
  </g>
  
  <!-- Satellite -->
  <g transform="rotate(10, 500, 200)">
    <rect x="480" y="190" width="40" height="25" fill="#e0f2fe" stroke="#7dd3fc" stroke-width="1"/>
    <rect x="470" y="195" width="10" height="15" fill="#7dd3fc"/>
    <rect x="520" y="195" width="10" height="15" fill="#7dd3fc"/>
    <rect x="485" y="180" width="30" height="10" fill="#7dd3fc"/>
    <circle cx="500" cy="200" r="3" fill="#f97316"/>
    <text x="500" y="230" font-family="Arial" font-size="14" text-anchor="middle" fill="white">Satellite</text>
  </g>
  
  <!-- Spaceship -->
  <g transform="rotate(20, 600, 300)">
    <path d="M 590 290 L 610 280 L 630 290 L 630 310 L 610 320 L 590 310 Z" fill="#e0f2fe" stroke="#7dd3fc" stroke-width="1"/>
    <circle cx="600" cy="300" r="8" fill="#f97316" filter="url(#glow)"/>
    <text x="610" y="330" font-family="Arial" font-size="14" text-anchor="middle" fill="white">Spacecraft</text>
  </g>
  
  <!-- Power modules -->
  <g>
    <!-- Power module 1 -->
    <rect x="150" y="350" width="60" height="40" rx="5" fill="#e6f0ff" stroke="#0072ce" stroke-width="2"/>
    <text x="180" y="375" font-family="Arial" font-size="12" text-anchor="middle" fill="#0072ce">DCM</text>
    
    <!-- Power module 2 -->
    <rect x="250" y="350" width="60" height="40" rx="5" fill="#e6f0ff" stroke="#0072ce" stroke-width="2"/>
    <text x="280" y="375" font-family="Arial" font-size="12" text-anchor="middle" fill="#0072ce">VTM</text>
    
    <!-- Power module 3 -->
    <rect x="150" y="420" width="60" height="40" rx="5" fill="#e6f0ff" stroke="#0072ce" stroke-width="2"/>
    <text x="180" y="445" font-family="Arial" font-size="12" text-anchor="middle" fill="#0072ce">PRM</text>
    
    <!-- Power module 4 -->
    <rect x="250" y="420" width="60" height="40" rx="5" fill="#e6f0ff" stroke="#0072ce" stroke-width="2"/>
    <text x="280" y="445" font-family="Arial" font-size="12" text-anchor="middle" fill="#0072ce">BCM</text>
  </g>
  
  <!-- Power connections -->
  <g stroke="#f97316" stroke-width="2">
    <line x1="180" y1="350" x2="180" y2="320"/>
    <line x1="280" y1="350" x2="280" y2="320"/>
    <line x1="180" y1="420" x2="180" y2="400"/>
    <line x1="280" y1="420" x2="280" y2="400"/>
    
    <!-- Connection between modules -->
    <line x1="210" y1="370" x2="250" y2="370"/>
    <line x1="210" y1="440" x2="250" y2="440"/>
  </g>
  
  <!-- Shield symbol for safety -->
  <path d="M 400 350 L 380 400 L 380 450 L 400 470 L 420 450 L 420 400 Z" fill="none" stroke="#10b981" stroke-width="3"/>
  <text x="400" y="400" font-family="Arial" font-size="14" text-anchor="middle" fill="#10b981">Safe</text>
  <text x="400" y="420" font-family="Arial" font-size="14" text-anchor="middle" fill="#10b981">Power</text>
  
  <!-- Reliability indicator -->
  <circle cx="600" cy="400" r="40" fill="none" stroke="#f97316" stroke-width="4" stroke-dasharray="5,5"/>
  <text x="600" y="405" font-family="Arial" font-size="14" text-anchor="middle" fill="#f97316">High</text>
  <text x="600" y="425" font-family="Arial" font-size="14" text-anchor="middle" fill="#f97316">Reliability</text>
  
  <text x="400" y="100" font-family="Arial" font-size="32" text-anchor="middle" fill="#e0f2fe">Aerospace Power Solutions</text>
  <text x="400" y="140" font-family="Arial" font-size="18" text-anchor="middle" fill="#bae6fd">High-Reliability Power for Extreme Environments</text>
  
  <!-- Efficiency indicator -->
  <g>
    <rect x="500" y="500" width="200" height="30" fill="none" stroke="#e0f2fe" stroke-width="2" rx="5"/>
    <rect x="500" y="500" width="180" height="30" fill="#d1fae5" rx="5"/>
    <text x="600" y="520" font-family="Arial" font-size="14" text-anchor="middle" fill="#065f46">Efficiency >96%</text>
  </g>
</svg>