<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <rect width="800" height="600" fill="#f8f9fa"/>
  <text x="400" y="180" font-family="Arial" font-size="36" text-anchor="middle" fill="#0072ce">Frequently Asked Questions</text>
  <text x="400" y="230" font-family="Arial" font-size="24" text-anchor="middle" fill="#6B7280">FAQ - Technical Support Center</text>
  
  <!-- Question and answer representation -->
  <g>
    <!-- Question bubble -->
    <rect x="200" y="280" width="400" height="60" rx="20" fill="white" stroke="#0072ce" stroke-width="2"/>
    <text x="240" y="315" font-family="Arial" font-size="16" fill="#0072ce">What is the difference between DCM and BCM modules?</text>
    
    <!-- Answer bubble -->
    <rect x="200" y="360" width="400" height="80" rx="20" fill="#e6f0ff" stroke="#0072ce" stroke-width="2"/>
    <text x="240" y="390" font-family="Arial" font-size="14" fill="#0072ce">DCM: Fixed-ratio, isolated converter without regulation</text>
    <text x="240" y="410" font-family="Arial" font-size="14" fill="#0072ce">BCM: Non-isolated, current multiplication with preserved dynamics</text>
    
    <!-- Question mark -->
    <circle cx="150" cy="310" r="30" fill="white" stroke="#f97316" stroke-width="3"/>
    <text x="150" y="320" font-family="Arial" font-size="32" text-anchor="middle" fill="#f97316">?</text>
    
    <!-- Check mark -->
    <circle cx="650" cy="400" r="30" fill="white" stroke="#10b981" stroke-width="3"/>
    <path d="M 635 400 L 645 410 L 665 390" stroke="#10b981" stroke-width="4" fill="none"/>
  </g>
  
  <!-- More Q&A items -->
  <g>
    <circle cx="220" cy="470" r="15" fill="white" stroke="#0072ce" stroke-width="2"/>
    <text x="220" y="475" font-family="Arial" font-size="18" text-anchor="middle" fill="#0072ce">Q</text>
    <rect x="240" y="460" width="280" height="25" rx="10" fill="#e2e8f9"/>
    <text x="250" y="478" font-family="Arial" font-size="14" fill="#0072ce">How efficient are Vicor modules?</text>
    
    <circle cx="220" cy="510" r="15" fill="white" stroke="#0072ce" stroke-width="2"/>
    <text x="220" y="515" font-family="Arial" font-size="18" text-anchor="middle" fill="#0072ce">Q</text>
    <rect x="240" y="500" width="280" height="25" rx="10" fill="#e2e8f9"/>
    <text x="250" y="518" font-family="Arial" font-size="14" fill="#0072ce">What input voltage ranges are supported?</text>
  </g>
  
  <!-- Support elements -->
  <g fill="none" stroke="#0072ce" stroke-width="2">
    <!-- Headset -->
    <circle cx="400" cy="100" r="25"/>
    <path d="M 370 110 Q 400 140 430 110" stroke="#0072ce"/>
    <rect x="385" y="85" width="30" height="10" fill="#0072ce"/>
  </g>
  
  <!-- Chat bubbles -->
  <g fill="#e6f0ff" stroke="#0072ce" stroke-width="1">
    <rect x="700" y="80" width="80" height="50" rx="10"/>
    <rect x="650" y="150" width="60" height="40" rx="10"/>
    <rect x="720" y="200" width="70" height="45" rx="10"/>
  </g>
  
  <g font-family="Arial" font-size="14" text-anchor="middle" fill="#0072ce">
    <text x="740" y="110">Quick</text>
    <text x="740" y="125">Support</text>
    <text x="680" y="175">24/7</text>
    <text x="755" y="228">Experts</text>
  </g>
  
  <!-- Icons -->
  <g fill="#f97316">
    <!-- Clock icon -->
    <circle cx="100" cy="550" r="15"/>
    <line x1="100" y1="540" x2="100" y2="550"/>
    <line x1="100" y1="550" x2="108" y2="550"/>
    
    <!-- Shield icon -->
    <path d="M 300 540 L 290 555 L 290 570 L 300 575 L 310 570 L 310 555 Z" fill="#10b981"/>
    
    <!-- Chip icon -->
    <rect x="500" y="540" width="20" height="20" fill="#0072ce"/>
    <rect x="525" y="545" width="5" height="5" fill="#f97316"/>
    <rect x="532" y="545" width="5" height="5" fill="#f97316"/>
    <rect x="525" y="552" width="5" height="5" fill="#f97316"/>
    <rect x="532" y="552" width="5" height="5" fill="#f97316"/>
  </g>
  
  <g font-family="Arial" font-size="12" text-anchor="middle" fill="#6B7280">
    <text x="100" y="580">Fast Response</text>
    <text x="300" y="580">Reliable</text>
    <text x="510" y="580">Technical</text>
  </g>
</svg>