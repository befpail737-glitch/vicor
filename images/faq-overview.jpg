<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <rect width="800" height="600" fill="#f9fafb"/>
  
  <!-- Background pattern -->
  <defs>
    <pattern id="circles" x="0" y="0" width="40" height="40" patternUnits="userSpaceOnUse">
      <circle cx="20" cy="20" r="1" fill="#d1d5db" opacity="0.4"/>
    </pattern>
  </defs>
  <rect width="800" height="600" fill="url(#circles)"/>
  
  <text x="400" y="100" font-family="Arial" font-size="28" text-anchor="middle" fill="#1f2937">Frequently Asked Questions</text>
  <text x="400" y="130" font-family="Arial" font-size="16" text-anchor="middle" fill="#6b7280">Common questions about Vicor power modules and applications</text>
  
  <!-- Question boxes -->
  <g>
    <!-- Question 1 -->
    <rect x="100" y="180" width="300" height="100" rx="12" fill="#ffffff" stroke="#d1d5db" stroke-width="2"/>
    <circle cx="130" cy="210" r="12" fill="#93c5fd"/>
    <text x="145" y="215" font-family="Arial" font-size="14" font-weight="bold" fill="#1e3a8a">Q</text>
    <text x="130" y="240" font-family="Arial" font-size="12" fill="#4b5563" font-weight="bold">Efficiency?</text>
    <text x="130" y="258" font-family="Arial" font-size="10" fill="#6b7280">How efficient are Vicor modules?</text>
    
    <!-- Answer 1 -->
    <rect x="100" y="290" width="300" height="50" rx="12" fill="#f0f9ff" stroke="#bae6fd" stroke-width="1"/>
    <circle cx="130" cy="320" r="12" fill="#34d399"/>
    <text x="145" y="325" font-family="Arial" font-size="14" font-weight="bold" fill="#06544a">A</text>
    <text x="130" y="345" font-family="Arial" font-size="10" fill="#06544a">Most Vicor modules exceed 94% efficiency,</text>
    <text x="130" y="358" font-family="Arial" font-size="10" fill="#06544a">with some reaching 96%+ under optimal conditions</text>
  </g>
  
  <g>
    <!-- Question 2 -->
    <rect x="400" y="180" width="300" height="100" rx="12" fill="#ffffff" stroke="#d1d5db" stroke-width="2"/>
    <circle cx="430" cy="210" r="12" fill="#93c5fd"/>
    <text x="445" y="215" font-family="Arial" font-size="14" font-weight="bold" fill="#1e3a8a">Q</text>
    <text x="430" y="240" font-family="Arial" font-size="12" fill="#4b5563" font-weight="bold">Thermal Design?</text>
    <text x="430" y="258" font-family="Arial" font-size="10" fill="#6b7280">What thermal considerations?</text>
    
    <!-- Answer 2 -->
    <rect x="400" y="290" width="300" height="50" rx="12" fill="#f0f9ff" stroke="#bae6fd" stroke-width="1"/>
    <circle cx="430" cy="320" r="12" fill="#34d399"/>
    <text x="445" y="325" font-family="Arial" font-size="14" font-weight="bold" fill="#06544a">A</text>
    <text x="430" y="345" font-family="Arial" font-size="10" fill="#06544a">Despite high efficiency, proper thermal</text>
    <text x="430" y="358" font-family="Arial" font-size="10" fill="#06544a">design is essential for reliability</text>
  </g>
  
  <g>
    <!-- Question 3 -->
    <rect x="100" y="370" width="300" height="100" rx="12" fill="#ffffff" stroke="#d1d5db" stroke-width="2"/>
    <circle cx="130" cy="400" r="12" fill="#93c5fd"/>
    <text x="145" y="405" font-family="Arial" font-size="14" font-weight="bold" fill="#1e3a8a">Q</text>
    <text x="130" y="430" font-family="Arial" font-size="12" fill="#4b5563" font-weight="bold">DCM vs BCM?</text>
    <text x="130" y="448" font-family="Arial" font-size="10" fill="#6b7280">Difference between DCM and BCM?</text>
    
    <!-- Answer 3 -->
    <rect x="100" y="480" width="300" height="50" rx="12" fill="#f0f9ff" stroke="#bae6fd" stroke-width="1"/>
    <circle cx="130" cy="510" r="12" fill="#34d399"/>
    <text x="145" y="515" font-family="Arial" font-size="14" font-weight="bold" fill="#06544a">A</text>
    <text x="130" y="535" font-family="Arial" font-size="10" fill="#06544a">DCM: Isolated, fixed-ratio; BCM: Non-</text>
    <text x="130" y="548" font-family="Arial" font-size="10" fill="#06544a">isolated, current multiplication</text>
  </g>
  
  <g>
    <!-- Question 4 -->
    <rect x="400" y="370" width="300" height="100" rx="12" fill="#ffffff" stroke="#d1d5db" stroke-width="2"/>
    <circle cx="430" cy="400" r="12" fill="#93c5fd"/>
    <text x="445" y="405" font-family="Arial" font-size="14" font-weight="bold" fill="#1e3a8a">Q</text>
    <text x="430" y="430" font-family="Arial" font-size="12" fill="#4b5563" font-weight="bold">Packaging?</text>
    <text x="430" y="448" font-family="Arial" font-size="10" fill="#6b7280">Which package is best?</text>
    
    <!-- Answer 4 -->
    <rect x="400" y="480" width="300" height="50" rx="12" fill="#f0f9ff" stroke="#bae6fd" stroke-width="1"/>
    <circle cx="430" cy="510" r="12" fill="#34d399"/>
    <text x="445" y="515" font-family="Arial" font-size="14" font-weight="bold" fill="#06544a">A</text>
    <text x="430" y="535" font-family="Arial" font-size="10" fill="#06544a">ChiP for max density, VIA for high</text>
    <text x="430" y="548" font-family="Arial" font-size="10" fill="#06544a">power &amp; thermal performance</text>
  </g>
  
  <!-- Decorative elements -->
  <g opacity="0.1">
    <circle cx="700" cy="100" r="60" fill="#3b82f6"/>
    <circle cx="100" cy="500" r="40" fill="#0072ce"/>
    <rect x="600" y="400" width="50" height="50" fill="#10b981" transform="rotate(45 600 400)"/>
  </g>
  
  <!-- Vicor logo representation -->
  <g>
    <rect x="350" y="550" width="100" height="30" rx="5" fill="#0072ce"/>
    <text x="400" y="570" font-family="Arial" font-size="14" text-anchor="middle" fill="white">Vicor</text>
  </g>
</svg>